    "def parse(line):\n",
    "    day_str, time_str, height_str = line.split()\n",
    "\n",
    "    day = int(day_str)\n",
    "    hour_str, minute_str = time_str.split(\":\")\n",
    "    hour_temp = float(hour_str)\n",
    "    minute = int(minute_str)\n",
    "\n",
    "    hour = hour_temp + (day * 24) + (minute/60)\n",
    "\n",
    "    height = float(height_str)\n",
    "\n",
    "    return hour, height"
   ]
  },
